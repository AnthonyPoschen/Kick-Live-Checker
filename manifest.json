{
  "manifest_version": 3,
  "name": "Kick Live Checker",
  "version": "1.0",
  "description": "Check if Kick.com streamer you follow are live",
  "permissions": ["cookies", "storage", "alarms"],
  "host_permissions": ["https://kick.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icons/icon.png"
  }
}
